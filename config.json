{
    "listeners":{
        "*:80":{
            "pass":"applications/weatherbot"
        },
        "*:443":{
            "pass":"applications/weatherbot",
            "tls":{
                "certificate": "certchain"
            }
        }
    },

    "applications": {
        "weatherbot":{
            "type":"python 3",
            "path": "/app",
            "home":"/opt/venv",
            "module":"weatherbot.wsgi"
        }
    }
}